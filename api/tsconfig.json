{
  "module": "commonjs",
  "target": "es5",
  "moduleResolution": "node",
  "experimentalDecorators": true,
  "emitDecoratorMetadata": true,
  "sourceMap": true,
  "declaration": false,
  "skipLibCheck": true,
  "stripInternal": true,
  "noImplicitAny": false,
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "models/*": [
        "server/models/*",
        "api/server/models/*"
      ],
      "collections/*": [
        "server/collections/*",
        "api/server/collections/*"
      ]
    },
    "typeRoots": [
      "node_modules/@types"
    ]
  }
}
